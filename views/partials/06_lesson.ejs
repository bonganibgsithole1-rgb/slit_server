<%if(slide){%>
<button class="slidingBtnsArrows compArrows">
  <svg id="leftArrow" viewBox="0 0 16 16">
    <path
      d="M8,0C3.582,0,0,3.582,0,8s3.582,8,8,8s8-3.582,8-8S12.418,0,8,0z M10.354,12.646l-0.707,0.707L4.293,8l5.354-5.354  l0.707,0.707L5.707,8L10.354,12.646z"
    ></path>
  </svg>
</button>
<!--  -->
<% if(slide.slide_content_type === 'to_do_list'){%>
<div class="subSection subSection1 subSection_Slides" id="<%=slide._id%>">
  <div class="imageDiv">
    <% if(slide.slide_type === 'video'){%>
    <iframe src="<%=slide.slide_type_url%>" frameborder="0"></iframe>
    <%}else{%>
    <img src="<%=slide.slide_type_url%>" alt="<%=slide.slide_type_url%>" />
    <%}%>
  </div>
  <h4 class="limittotwo"><%=slide.slide_content_title%></h4>
  <ol>
    <% slide.slide_to_do_list_arr.forEach(function(to_do_list){%>
    <li><%=to_do_list%></li>
    <%})%>
  </ol>
</div>
<%}%>
<!--  -->
<% if(slide.slide_content_type === 'slide_video_image_view'){%>
<div class="subSection subSection2 subSection_Slides" id="<%=slide._id%>">
  <p class="limittofour"><%=slide.slide_body%></p>
  <div class="imageDiv">
    <% if(slide.slide_type === 'video'){%>
    <iframe src="<%=slide.slide_type_url%>" frameborder="0"></iframe>
    <%}else{%>
    <img src="<%=slide.slide_type_url%>" alt="<%=slide.slide_type_url%>" />
    <%}%>
  </div>
</div>
<%}%>
<!--  -->
