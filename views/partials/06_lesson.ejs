<% if(slide.slide_content_type === 'summary'){%>
<div
  class="subSection subSection3 subSection4 subSection5 subSection_Slides"
  id="<%=slide._id%>"
>
  <div class="imageDiv_3 imageDiv imageDiv_4">
    <img src="<%=slide.slide_type_url%>" />
  </div>
  <div class="halfContent">
    <h3>Lesson Summary</h3>
    <ol class="halfLinks">
      <% slide.slide_to_do_list_arr.forEach(function(list){%>
      <li class="limittotwo"><%=list%></li>
      <%})%>
    </ol>
  </div>
</div>
<%}%>
